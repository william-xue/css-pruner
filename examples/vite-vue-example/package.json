{
  "name": "vite-vue-example",
  "version": "1.0.0",
  "description": "Example Vue app with Vite and CSS Pruner",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "css-analyze": "css-pruner analyze --config ./css-pruner.config.js",
    "css-clean": "css-pruner clean --config ./css-pruner.config.js --backup --verbose",
    "build:css":"npm run build -- --config vite.config.plugin.js"
  },
  "dependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@fe-fast/unused-css-pruner": "file:../..",
    "@types/css-tree": "^2.3.10",
    "@vitejs/plugin-vue": "^4.2.0",
    "vite": "^4.4.0"
  }
}