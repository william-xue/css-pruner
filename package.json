{
  "name": "@fe-fast/unused-css-pruner",
  "version": "1.0.1",
  "description": "A powerful CSS pruning tool that removes unused styles with support for dynamic class names, CSS-in-JS, and component-level analysis",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "css-pruner": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vite": {
      "import": "./dist/vite.js",
      "types": "./dist/vite.d.ts"
    },
    "./webpack": {
      "import": "./dist/webpack.js",
      "types": "./dist/webpack.d.ts"
    },
    "./rollup": {
      "import": "./dist/rollup.js",
      "types": "./dist/rollup.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "css",
    "unused",
    "pruning",
    "optimization",
    "tailwind",
    "css-in-js",
    "postcss",
    "build-tool"
  ],
  "author": "william-xue",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/william-xue/css-pruner.git"
  },
  "homepage": "https://github.com/william-xue/css-pruner",
  "bugs": {
    "url": "https://github.com/william-xue/css-pruner/issues"
  },
  "files": [
    "dist",
    "README.md",
    "README_CN.md"
  ],
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "css-tree": "^3.1.0",
    "glob": "^11.0.3",
    "postcss": "^8.5.6",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/glob": "^9.0.0",
    "@types/node": "^24.1.0"
  }
}
